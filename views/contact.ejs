<% include partials/header.ejs %>

    <!--MAIN CONTENT -->
    <main class="container">

        <h1><%= title %></h1>
        <p>
            If you have any questions or need to contact us for any 
            reason, feel free to send us an email. Please allow up to 2 business days for a response.
        </p>

        <form>
            <div class="form-group">
                <label for="emailInput">Email address</label>
                <input type="email" class="form-control" id="emailInput" placeholder="Email">
            </div>
            <div class="form-group">
                <label for="fileAttachment">File input</label>
                <input type="file" id="fileAttachment">
                <p class="help-block">This email will not send to any actual address.</p>
            </div>
            <button type="submit" class="btn btn-default">Submit</button>
        </form>

    </main>

    <% include partials/footer.ejs %>